#include <iostream>

using namespace std;

int main() {
    int n, m;
    cin >> n >> m;

    int a[100][100]; 
    int top = 0, bottom = n - 1, left = 0, right = m - 1;
    int num = 1;

    while (top <= bottom && left <= right) {
        for (int i = left; i <= right; i++) {
            a[top][i] = num++;
        }
        top++;

        // Move down
        if (top <= bottom) {
            for (int i = top; i <= bottom; i++) {
                a[i][right] = num++;
            }
            right--;
        }

        if (left <= right && top <= bottom) {
            for (int i = right; i >= left; i--) {
                a[bottom][i] = num++;
            }
            bottom--;
        }

        if (left <= right && top <= bottom) {
            for (int i = bottom; i >= top; i--) {
                a[i][left] = num++;
            }
            left++;
        }
    }

    for (int i = 0; i < n; i++) {
        for (int j = 0; j < m; j++) {
            cout << a[i][j] << " ";
        }
        cout << endl;
    }
    return 0;
}

